{
  "source": "./",
  "build": "dist/",
  "domain": "insomnia.rest",
  "defaults": {
    "^/.*": {
      "_layout": "default.html",
      "_title": "Insomnia"
    },
    "^/index.html$": {
      "page": {
        "type": "splash"
      }
    },
    "^/documentation/index.html$": {
      "_title": "Documentation"
    },
    "^/blog/index.html$": {
      "_title": "Changelog"
    },
    "^/blog/.*$": {
      "_title": "Changelog",
      "page": {
        "type": "blog_post"
      }
    },
    "^/documentation/.*$": {
      "_title": "Documentation",
      "page": {
        "type": "doc"
      }
    }
  }
}
